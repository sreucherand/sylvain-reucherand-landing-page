{
  "name": "sylvain-reucherand-landing-page",
  "version": "1.0.0",
  "main": "server.js",
  "author": "Sylvain Reucherand <hello@sylvainreucherand.fr>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.55",
    "@babel/preset-react": "^7.0.0-beta.55",
    "autoprefixer": "^9.0.2",
    "babel-loader": "^8.0.0-beta",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "postcss-loader": "^2.1.6",
    "postcss-nested": "^3.0.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "scripts": {
    "build": "npm run build:css && npm run build:js && npm run build:views",
    "build:css": "stylus ./src/app.styl --include node_modules/ --include-css --print | cssnano > ./public/static/app.bundle.css",
    "build:js": "webpack app='./src/app.js' --module-bind js=babel-loader --output-path='./public/static' --output-filename='[name].bundle.js' -p --progress",
    "build:views": "pug ./src/views/index.pug --obj ./src/data/expose.js --out ./public/",
    "eslint": "eslint src/**/*.js; exit 0",
    "prebuild": "rm -rf public/ && mkdir -p public/static",
    "prestart": "rm -rf public/ && mkdir -p public/static",
    "server": "nodemon --watch ./server.js --watch ./src/data server.js",
    "start": "HOT=1 NODE_ENV=development webpack-dev-server --config webpack.config.development.js",
    "test": "echo \"Error: no test specified\" && exit 0",
    "watch:css": "stylus ./src/app.styl --include node_modules/ --include-css --out ./public/static/app.bundle.css --use autoprefixer-stylus --watch",
    "watch:js": "webpack app='./src/app.js' --module-bind js=babel-loader --output-path='./public/static' --output-filename='[name].bundle.js' --watch"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "dom-event": "^1.0.0",
    "emptyfunction": "^1.0.0",
    "escape-html": "^1.0.3",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.10",
    "mime-types": "^2.1.19",
    "normalize.css": "^8.0.0",
    "numeral": "^2.0.6",
    "prismic-javascript": "^1.5.0",
    "prismic-richtext": "^0.4.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "rebound": "^0.1.0"
  }
}
